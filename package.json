    "url": "https://github.com/Nirbagi/isr-swimming-app/issues"
  },
  "homepage": "https://github.com/Nirbagi/isr-swimming-app#readme",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.1",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.0",
    "enforce-node-path": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.3",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-joi-router": "^8.0.0",
    "koa-morgan": "^1.0.1",
    "koa-passport": "^4.1.4",
